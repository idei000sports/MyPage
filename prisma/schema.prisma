generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model notes {
  id        Int      @id @default(autoincrement())
  content   String
  createdAt DateTime @default(now())
}

model Origin {
  id          Int      @id @default(autoincrement())
  name        String
  country     String
  state       String
  county      String
  city        String
  formation   Int
  dissolution Int
  genres      String[]
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@unique([name, county])
}

model CityName {
  id        Int    @id @default(autoincrement())
  country   String
  state     String
  county    String
  lattitude Float
  longitude Float

  @@unique([country, state, county])
}
