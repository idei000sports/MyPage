// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model notes {
  id        Int      @id @default(autoincrement())
  content   String
  createdAt DateTime @default(now())
}

model Origin {
  id        Int       @id @default(autoincrement())
  name      String
  country String
  state String
  county String
  formation     Int
  dissolution       Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

} 

model City {
    id        Int       @id @default(autoincrement())
    country String
    state String
    county String


    lattitude Float
    longitude Float

}